{
  "name": "gdg-pup-platform",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "start": "turbo run start",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "build:nexus": "turbo build --filter=@packages/identity-api-contracts --filter=@packages/nexus-api-contracts --filter=nexus-api --filter=identity-api --filter=nexus-web",
    "dev:nexus": "turbo dev --filter=@packages/identity-api-contracts --filter=@packages/nexus-api-contracts --filter=nexus-api --filter=identity-api --filter=nexus-web",
    "gen-types": "turbo run gen-types",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "prettier": "3.7.4",
    "syncpack": "13.0.4",
    "turbo": "2.7.4",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0"
  }
}
