{
  "name": "@packages/identity-api-contracts",
  "exports": {
    ".": "./dist/typedrest.contract.js"
  },
  "imports": {
    "#*": "./dist/*"
  },
  "scripts": {
    "gen-types:supabase": "supabase gen types typescript --project-id xrsmassoekhstkeuwozc > src/types/supabase.types.ts",
    "gen-types:zod": "supazod -i src/types/supabase.types.ts -o src/types/supabase.schema.ts",
    "gen-types": "pnpm run gen-types:supabase && pnpm run gen-types:zod",
    "gen-contract": "typed-rest",
    "dev": "concurrently \"typed-rest --watch\" \"tsc --watch\"",
    "build": "typed-rest && tsc"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@packages/typed-rest": "workspace:*",
    "concurrently": "9.2.1",
    "supazod": "^4.5.0",
    "zod": "^4.3.2"
  },
  "devDependencies": {
    "@configs/typescript-config": "workspace:*",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "supabase": "^2.70.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
